<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>dark</theme>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>

        <!-- title of the document -->
        <documentTitle>INSIDER TEST REPORT</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>
            <image id="image-zoom">

            </image>
        </reportName>


        <!-- location of charts in the test view -->
        <!-- top, bottom -->
        <testViewChartLocation>bottom</testViewChartLocation>

        <!-- custom javascript -->
        <scripts>
            <![CDATA[
    $(document).ready(function() {
      $("#image-zoom").src("src",src);
	  $("#image-zoom").append("src",src);
    });
  ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[
            .report-name { padding-left: 10px; } .report-name > img { float:
      left;height: 30%;margin-left: 30px;margin-top: 1px;width: auto; }
        ]]>
        </styles>
    </configuration>
</extentreports>